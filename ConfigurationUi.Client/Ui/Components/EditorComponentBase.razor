@code {
    
    /// <summary>
    /// Current configuration token, which should be rendered by this component
    /// </summary>
    [Parameter]
    [EditorRequired]
    public JToken Token { get; set; }
    
    
    
    /// <summary>
    /// Key of element in parent
    /// </summary>
    [Parameter]
    [EditorRequired]
    public string Key { get; set; }

    /// <summary>
    /// Schema for current configuration section
    /// </summary>
    [Parameter]
    [EditorRequired]
    public JsonSchema4 Schema { get; set; }
}